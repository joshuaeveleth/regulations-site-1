extends: airbnb

globals:
    document: true
    localStorage: true
    sessionStorage: true
    window: true
    XMLHttpRequest: true
    $: true
# i.e. fixmes
rules:
    no-restricted-syntax: off
    no-shadow: off
    no-underscore-dangle: off
    no-unused-vars: off
    no-var: off
    object-curly-spacing: off
    object-shorthand: off
    one-var-declaration-per-line: off
    one-var: off
    operator-assignment: off
    padded-blocks: off
    prefer-arrow-callback: off
    prefer-rest-params: off
    prefer-template: off
    quote-props: [error, consistent]
    space-before-function-paren: off
    space-in-parens: off
    space-infix-ops: off
    space-unary-ops: off
    strict: off
    vars-on-top: off
